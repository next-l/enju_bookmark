= form_for(@bookmark_stat) do |f|
  = f.error_messages
  .form-group
    = f.label :start_date
    br
    = f.datetime_select :start_date
  .form-group
    = f.label :end_date
    br
    = f.datetime_select :end_date
  .form-group
    = f.label :note
    br
    = f.text_area :note
  .form-control
    = f.submit
